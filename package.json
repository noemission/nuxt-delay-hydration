{
  "name": "@noemission/nuxt-delay-hydration",
  "type": "module",
  "version": "1.0.1",
  "description": "Improve your Nuxt.js Google Lighthouse score by delaying hydration ⚡️",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "hydration",
    "lighthouse",
    "page speed",
    "performance"
  ],
  "exports": {
    "types": "./dist/module.d.ts",
    "import": "./dist/module.mjs",
    "require": "./dist/module.cjs"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "build": {
    "entries": [
      {
        "builder": "mkdist",
        "input": "./src/template/"
      }
    ]
  },
  "scripts": {
    "dev": "nuxi dev .playground",
    "stub": "nuxt-module-build build --stub",
    "build": "nuxi prepare .playground && nuxt-module-build build",
    "lint": "eslint . --fix",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "true"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.1",
    "lodash-es": "^4.17.21",
    "node-html-parser": "^6.1.13",
    "packrup": "^0.1.0",
    "radix3": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.1",
    "@nuxt/test-utils": "^3.8.1",
    "@nuxt/types": "^2.17.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "^20.9.0",
    "@vitest/ui": "^0.34.6",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "nuxt": "^3.8.1",
    "typescript": "^5.2.2",
    "upath": "^2.0.1",
    "vitest": "^0.34.6"
  }
}
